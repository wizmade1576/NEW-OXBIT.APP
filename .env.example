# Supabase 환경 변수 (Vite는 VITE_ 접두사 필요)
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=
# Enable calling Supabase Edge Functions from the client (optional)
# Set to 'true' only if the `news-proxy` function is deployed in your project
VITE_USE_EDGE_FUNCTIONS=true

# Enable verbose auth logs in dev
VITE_DEBUG_AUTH=false
